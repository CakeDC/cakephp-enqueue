{
    "name": "cakedc/cakephp-enqueue",
    "description": "CakephpEnqueue plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "cakephp/queue": "^2.0",
        "cakephp/cakephp": "^5.1",
		"cakephp/migrations": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "enqueue/enqueue": "^0.10.15",
        "enqueue/fs": "^0.10",
        "enqueue/simple-client": "^0.10",
        "psr/log": "^3.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Cake\\Enqueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Enqueue\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "TestApp\\": "tests/test_app/src/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check"
        ],
        "cs-check": "phpcs -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "cs-fix": "phpcbf --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "test": "phpunit --stderr"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
